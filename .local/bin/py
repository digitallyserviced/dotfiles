#!/usr/bin/python

import re
import sys

per_line = sys.argv[1] == '-l'
stdin = [line.strip() for line in sys.stdin.readlines()]

if per_line:
    code = ' '.join(sys.argv[2:]).replace('{}', 'line')
    for line in stdin:
        print(eval(code))
else:
    code = ' '.join(sys.argv[1:]).replace('{}', 'stdin')
    for line in eval(code):
        print(line)
