	"[38;2;137;239;250m╰───────────────────────────────────────────────────╯[38;2;115;227;249m╰┘[1m[38;2;175;213;222m↑[38;2;88;123;123m select [38;2;175;213;222m↓[0m[38;2;115;227;249m[48;2;15;28;30m└┘[1m[38;2;88;123;123minfo [38;2;175;213;222m↵[0m[38;2;115;227;249m[48;2;15;28;30m└┘[1m[38;2;175;213;222mt[38;2;88;123;123merminate[0m[38;2;115;227;249m[48;2;15;28;30m└┘[1m[38;2;175;213;222mK[38;2;88;123;123mill[0m[38;2;115;227;249m[48;2;15;28;30m└┘[1m[38;2;175;213;222ms[38;2;88;123;123mignals[0m[38;2;115;227;249m[48;2;15;28;30m└─────────┘[1m[38;2;88;123;123m2/379[0m[38;2;115;227;249m[48;2;15;28;30m└╯\n")

matches = re.finditer(regex, test_str, re.MULTILINE | re.UNICODE )

for matchNum, match in enumerate(matches, start=1):
    
    print ("Match {matchNum} was found at {start}-{end}: {match}".format(matchNum = matchNum, start = match.start(), end = match.end(), match = match.group()))
    
    for groupNum in range(0, len(match.groups())):
        groupNum = groupNum + 1
        
        print ("Group {groupNum} found at {start}-{end}: {group}".format(groupNum = groupNum, start = match.start(groupNum), end = match.end(groupNum), group = match.group(groupNum)))

# Note: for Python 2.7 compatibility, use ur"" to prefix the regex and u"" to prefix the test string and substitution.

